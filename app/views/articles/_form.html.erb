<%= form_for(@article) do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :url %>
	<%= f.text_field :url %>

	<%= f.label :publication %>
	<%= f.text_field :publication %>

	<%= f.label :tags %>
	<%= f.text_field :tags %>

	<%= f.label :text %>
	<%= f.text_field :text %>

	<%= f.label :score %>
	<%= f.text_field :score %>

	<%= f.submit %>
<% end %>